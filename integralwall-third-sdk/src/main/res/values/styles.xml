<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="umeng_style_dialog_title">
        <item name="android:background">@drawable/shape_dialog_title_umeng</item>
        <item name="android:layout_height">41dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#292931</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:padding">10dp</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="umeng_style_user_login">
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@drawable/selector_font_white_gray_umeng</item>
    </style>

    <style name="umeng_style_user_login.register" parent="umeng_style_user_login">
        <item name="android:background">@drawable/selector_common_red_umeng</item>
    </style>

    <style name="umeng_style_user_login.me" parent="umeng_style_user_login">
        <item name="android:background">@drawable/selector_common_black_umeng</item>
    </style>

</resources>